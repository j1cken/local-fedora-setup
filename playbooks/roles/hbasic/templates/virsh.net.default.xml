<network>
  <name>default</name>
  <forward mode='nat'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <bridge name='virbr0' stp='on' delay='0'/>
  <domain name='{{domain}}'/>
  <mac address='{{mac}}'/>
  <ip address='192.168.{{subnet}}.1' netmask='255.255.255.0'>
    <dhcp>
      <range start='192.168.{{subnet}}.3' end='192.168.{{subnet}}.254'/>
    </dhcp>
  </ip>
</network>
